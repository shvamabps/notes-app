    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.7.0",
    "morgan": "1.10.0",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.2"
